{
  "name": "warden-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "spellcheck": "mdspell --ignore-acronyms --ignore-numbers --en-us '**/*.md' '!**/node_modules/**/*.md'",
    "spellcheck:report": "mdspell --ignore-acronyms --ignore-numbers --en-us --report '**/*.md' '!**/node_modules/**/*.md'",
    "lint": "npm run spellcheck:report"
  },
  "dependencies": {
    "@docusaurus/core": "3.6.0",
    "@docusaurus/plugin-google-gtag": "3.5.1",
    "@docusaurus/plugin-google-tag-manager": "3.5.1",
    "@docusaurus/preset-classic": "3.6.0",
    "@docusaurus/theme-mermaid": "3.6.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.5.1",
    "@docusaurus/tsconfig": "3.5.1",
    "@docusaurus/types": "3.5.1",
    "markdown-spellcheck": "^1.3.1",
    "typescript": "~5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
